{
  "name": "adgenxai",
  "version": "1.0.0",
  "description": "AI-powered advertising solution with webhook sensory cortex",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "deploy": "netlify deploy --prod",
    "test": "vitest run --reporter=dot",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=dot",
    "test:integration": "node tests/integration/postSignedWebhook.js",
    "test:syntax": "Get-ChildItem -Path . -Include *.js -Recurse | Where-Object { $_.FullName -notmatch 'node_modules' } | ForEach-Object { node -c $_.FullName }",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "check:corruption": "node scripts/check-corruption.js",
    "triage:prs": "node scripts/pr-triage.mjs",
    "setup:agents": "node setup-agents.mjs",
    "agent:deploy": "github-agent deploy",
    "agent:health": "github-agent health --detailed",
    "agent:status": "github-agent status",
    "agent:monitor": "github-agent monitor --interval 30"
  },
  "dependencies": {
    "@netlify/blobs": "^7.0.0",
    "@netlify/functions": "^2.4.0",
    "@next/swc-win32-x64-msvc": "^16.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "apexcharts": "^4.7.0",
    "axios": "^1.6.0",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "express": "^4.18.2",
    "framer-motion": "^11.0.0",
    "github-agent-cli": "^1.0.1",
    "googleapis": "^131.0.0",
    "ioredis": "^5.3.0",
    "lucide-react": "^0.263.1",
    "next": "^16.0.1",
    "prom-client": "^15.1.0",
    "react": "^18.3.0",
    "react-apexcharts": "^1.8.0",
    "react-dom": "^18.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.4"
  },
  "keywords": [
    "ai",
    "advertising",
    "webhook",
    "mcp",
    "sensory-cortex"
  ],
  "author": "brandonlacoste9-tech",
  "license": "MIT",
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.52.5"
  }
}

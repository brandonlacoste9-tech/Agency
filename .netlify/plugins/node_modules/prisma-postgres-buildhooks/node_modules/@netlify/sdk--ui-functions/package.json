{
  "name": "@netlify/sdk--ui-functions",
  "version": "1.6.1",
  "author": "Netlify, Inc.",
  "description": "A toolkit for building Netlify Functions for use by UI extensions.",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./trpc": {
      "types": "./dist/trpc/main.d.ts",
      "default": "./dist/trpc/main.js"
    }
  },
  "dependencies": {
    "@netlify/functions": "^3.0.0",
    "zod": "^3.23.8",
    "@netlify/sdk--extension-api-client": "2.5.0"
  },
  "devDependencies": {
    "@trpc/server": "11.0.0-rc.700",
    "@types/node": "^22.10.6",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "@trpc/server": ">= 11 || >= 11.0.0-rc.0"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "watch": "pnpm run build:watch",
    "clean": "rm -rf dist/ && rm -f *.tsbuildinfo",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  }
}